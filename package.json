{
  "name": "sns_app",
  "version": "1.0.0",
  "description": "- **🌐 Chrome Web Support**: Full web browser compatibility\r - **🎬 AI YouTube Videos**: 17 curated AI videos with interactive playback\r - **✨ Enhanced Animations**: Pulse animations, smooth transitions, and interactive icons\r - **🔍 Smart Search**: Video search and category filtering\r - **📱 Multi-Platform**: Android, Windows, and Web support\r - **🤖 Playwright MCP Integration**: Automated browser testing and web automation",
  "main": "simple_test.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:codegen": "playwright codegen http://localhost:3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bskang7777/sns_app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bskang7777/sns_app/issues"
  },
  "homepage": "https://github.com/bskang7777/sns_app#readme",
  "devDependencies": {
    "@playwright/test": "^1.54.1"
  }
}
